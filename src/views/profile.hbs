<h1>Profile</h1>
<section>
  <div class="userImage">
    <img src="any" alt="user image">
  </div>

  <div class="userName">
    <h2>{{username}}</h2>
  </div>

<div class="aboutDiv">
  <h2>About</h2>
  <p>{{About}}</p>
</div>

<div class="skills">
  <h2>Skills</h2>
  <ul>
    <li>{{skills}}</li>
  </ul>
</div>

<div class="contact">
<h2>Contact</h2>
<ul>
  <li>{{contact}}</li>
</ul>
</div>


</section>
